{
  "name": "pandas-learning-platform",
  "version": "1.0.0",
  "description": "Full-stack pandas learning platform with authentication and progress tracking",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:frontend": "cd frontend && npm run start",
    "start:backend": "cd backend && npm run start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "db:init": "cd database/mysql && mysql -u root -p < init.sql",
    "db:seed": "cd backend && npm run seed"
  },
  "keywords": [
    "pandas",
    "data-science",
    "learning",
    "nextjs",
    "express",
    "mysql",
    "mongodb"
  ],
  "author": "Pandas Learning Platform Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "multer": "^2.0.2"
  }
}
